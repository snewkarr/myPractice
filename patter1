// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n=8,m=11;
    for(int i=0;i<n;i++)
    {
        
        {
            if(i==0 || i==7 )
            {
                for(int j=0;j<m;j++)
                {
                    printf("*");
                }
            }
            else{
                for(int j=0;j<m;j++)
                {
                    if(j==0 || j==5)
                    {
                        printf("*");
                    }
                    else{
                        printf(" ");
                    }
                }
            }
            
        }
        printf("\n");
    }

    return 0;
}
